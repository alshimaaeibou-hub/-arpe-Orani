#include <stdio.h>

int main() {
    double getiri, risksizFaiz, risk, sharpeOrani;

    printf("Yatirim getirisini (%% olarak) giriniz: ");
    scanf("%lf", &getiri);

    printf("Risksiz faiz oranini (%% olarak) giriniz: ");
    scanf("%lf", &risksizFaiz);

    printf("Portfoyun standart sapmasini (%% olarak) giriniz: ");
    scanf("%lf", &risk);

 
    sharpeOrani = (getiri - risksizFaiz) / risk;

    printf("\nSharpe Orani = %.2f\n", sharpeOrani);

    if (sharpeOrani > 3)
        printf("Bu deger mukemmel kabul edilir.\n");
    else if (sharpeOrani > 2)
        printf("Bu deger cok iyi kabul edilir.\n");
    else if (sharpeOrani > 1)
        printf("Bu deger iyi kabul edilir.\n");
    else
        printf("Bu deger dusuk kabul edilir.\n");

  
    printf("\nPress Enter to exit...");
    getchar(); 
    getchar();
    return 0;
}

	

 
